ge + '/' + this.size, {\n        params: {\n          userId: this.param.userId\n        }\n      }).then(function (res) {\n        _this.isload = false;\n        var data = res.data;\n\n        if (data.success) {\n          _this.queryResult = data.queryResult.list;\n          console.log(_this.queryResult);\n        }\n      }).catch(function (err) {\n        console.log(err);\n      });\n    }\n  }\n};",{"version":3,"sources":["mycollect.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;AAiBA,OAAA,KAAA,MAAA,OAAA;;;AAEA,OAAA,SAAA;AACA,OAAA,OAAA,MAAA,0BAAA;AACA,OAAA,OAAA,MAAA,eAAA;AACA,SAAA,SAAA,QAAA,IAAA;AACA,eAAA;AACA,EAAA,IAAA,EAAA,WADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,SAAA,EAAA,SADA;AAEA,IAAA,OAAA,EAAA;AAFA,GAFA;AAMA,EAAA,IAAA,EAAA,gBAAA;AACA,WAAA;AACA,MAAA,KAAA,EAAA,MADA;AAEA,MAAA,IAAA,EAAA,IAFA;AAGA,MAAA,MAAA,EAAA,IAHA;AAIA,MAAA,IAAA,EAAA,CAJA;AAKA,MAAA,IAAA,EAAA,CALA;AAMA,MAAA,WAAA,EAAA,EANA;AAOA,MAAA,KAAA,EAAA;AACA,QAAA,IAAA,EAAA,CADA;AAEA,QAAA,IAAA,EAAA,CAFA;AAGA,QAAA,MAAA,EAAA;AAHA;AAPA,KAAA;AAaA,GApBA;AAqBA,EAAA,OArBA,qBAqBA;AACA,SAAA,KAAA,CAAA,MAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,QAAA,EAAA,MAAA;AACA,SAAA,OAAA;AACA,GAxBA;AAyBA,EAAA,OAAA,EAAA;AACA,IAAA,OADA,qBACA;AAAA;;AACA,MAAA,KAAA,CACA,GADA,CACA,8DAAA,KAAA,IAAA,GAAA,GAAA,GAAA,KAAA,IADA,EACA;AACA,QAAA,MAAA,EAAA;AACA,UAAA,MAAA,EAAA,KAAA,KAAA,CAAA;AADA;AADA,OADA,EAMA,IANA,CAMA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,MAAA,GAAA,KAAA;AACA,YAAA,IAAA,GAAA,GAAA,CAAA,IAAA;;AACA,YAAA,IAAA,CAAA,OAAA,EAAA;AACA,UAAA,KAAA,CAAA,WAAA,GAAA,IAAA,CAAA,WAAA,CAAA,IAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,WAAA;AACA;AACA,OAbA,EAcA,KAdA,CAcA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAhBA;AAiBA;AAnBA;AAzBA,CAAA","sourcesContent":["<template>\n  <div class=\"collect-page\">\n      <headtitle :title=\"title\" :back=\"true\"></headtitle>\n      <router-link class=\"collect-item\" :to=\"{path:'/detail', query: {id:item.id}}\" tag=\"div\" v-for=\"item in queryResult\" :key=\"item.id\">\n         <div class=\"collect-img-box\">\n            <img class=\"collect-img\" :src=\"item.videoImageUrl\" alt=\"\">\n          </div>\n          <div class=\"collect-info\">\n             <div class=\"collect-name\">{{item.videoName}}</div>\n             <div>{{item.videoPlayTime}}</div>\n          </div>\n      </router-link>\n      <loading :isload=\"isload\"></loading>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport {Style,Slide} from 'cube-ui'\nimport headtitle from '../title.vue'\nimport loading from '@/components/loading.vue'\nimport BScroll from 'better-scroll'\nimport { constants } from 'fs';\nexport default {\n  name: \"mycollect\",\n  components:{\n    headtitle,\n    loading\n  },\n  data: function(){\n      return {\n        title:'收藏列表',\n        back: true,\n        isload: true,\n        page: 1,\n        size: 6,\n        queryResult: [],\n        param:{\n            page:1,\n            size:2,\n            userId:''\n        }\n      }\n  },\n  mounted(){\n      this.param.userId = JSON.parse(this.$store.state.userinfo).userId\n      this.getList();\n  },\n  methods:{\n    getList () {\n        axios\n        .get('http://iot.smart-ideas.com.cn/ssgy/lnVideo/listUserVideo/'+this.page+'/'+this.size,{\n            params: {\n                userId: this.param.userId \n            }\n        })\n        .then((res)=>{\n            this.isload = false;\n            var data = res.data;\n            if(data.success){\n                this.queryResult = data.queryResult.list;\n                console.log(this.queryResult)\n            }\n        })\n        .catch((err) => {\n            console.log(err)\n        })\n    },\n  }\n};\n</script>\n<style lang=\"less\" scoped>\n.collect-page {\n    padding: 50px 15px 10px;\n    .collect-item{\n        padding: 15px 0;\n        display: flex;\n        .collect-img-box{\n            width: 120px;\n            height: 100px;\n            overflow: hidden;\n            position: relative;\n            .collect-img{\n                position: relative;\n                top:50%;\n                left:50%;\n                transform: translate(-50%,-50%);\n                transform-origin:50% 50%;\n                display: block;\n                width: auto;\n                height: 100px;\n            }\n        }\n        .collect-info{\n            margin-left:15px;\n            flex: 1;\n            .collect-name{\n                height: 48px;\n                line-height: 24px;\n                overflow: hidden;\n                font-size: 16px;\n                margin-bottom: 4px;\n            }\n        }\n        \n    }\n}\n</style>\n"],"sourceRoot":"src/views/mine"}]}