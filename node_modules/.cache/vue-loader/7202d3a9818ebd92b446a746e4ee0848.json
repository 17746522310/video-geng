        .get('http://iot.smart-ideas.com.cn/ssgy/lnVideo/listUserVideo/'+this.page+'/'+this.size,{\n            params: {\n                userId: this.param.userId \n            }\n        })\n        .then((res)=>{\n            this.isload = false;\n            var data = res.data;\n            if(data.success){\n                this.queryResult = data.queryResult.list;\n                console.log(this.queryResult)\n            }\n        })\n        .catch((err) => {\n            console.log(err)\n        })\n    },\n  }\n};\n</script>\n<style lang=\"less\" scoped>\n.collect-page {\n    padding: 50px 15px 10px;\n    .collect-item{\n        padding: 15px 0;\n        display: flex;\n        .collect-img-box{\n            width: 120px;\n            height: 100px;\n            overflow: hidden;\n            position: relative;\n            .collect-img{\n                position: relative;\n                top:50%;\n                left:50%;\n                transform: translate(-50%,-50%);\n                transform-origin:50% 50%;\n                display: block;\n                width: auto;\n                height: 100px;\n            }\n        }\n        .collect-info{\n            margin-left:15px;\n            flex: 1;\n            .collect-name{\n                height: 48px;\n                line-height: 24px;\n                overflow: hidden;\n                font-size: 16px;\n                margin-bottom: 4px;\n            }\n        }\n        \n    }\n}\n</style>\n"]}]}